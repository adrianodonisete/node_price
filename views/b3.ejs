<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <% if (objStock['Global Quote'] === undefined) { %>
    <h1>Erro! Não foi possível encontrar a cotação para esse ativo.</h1>
    <p>
        <%= objStock['Error Message'] %>
    </p>
    <% } else { %>
    <h1>Cotação <%= objStock['Global Quote']['01. symbol'] %></h1>
    <p>
        R$ <%= objStock['Global Quote']['05. price'] %>
    </p>
    <% } %>
  </body>
</html>

